{
  "configurations": [
    {
      "name": "Pico",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/../pico-sdk/**",
        "${workspaceFolder}/../pico-sdk/src/common/include",
        "${workspaceFolder}/../pico-sdk/src/boards/include",
        "${workspaceFolder}/../pico-sdk/src/rp2_common/pico_stdlib/include",
        "${workspaceFolder}/../pico-sdk/src/rp2040/hardware_regs/include",
        "${workspaceFolder}/../pico-sdk/src/rp2_common/hardware_gpio/include"
      ],
      "defines": [
        "PICO_ON_DEVICE=1"
      ],
      "compilerPath": "/usr/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "limitSymbolsToIncludedHeaders": true,
        "path": [
          "${workspaceFolder}/**",
          "${workspaceFolder}/../pico-sdk/**"
        ]
      }
    }
  ],
  "version": 4
}
